{
  "name": "my-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && node ./bin/www",
    "dev": "tsc -w & nodemon ./bin/www",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "format": "eslint 'src/**/*.ts' --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "debug": "^2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.16.1",
    "http-errors": "^1.6.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "jest": "^29.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
